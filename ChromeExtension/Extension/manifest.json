{

  "manifest_version": 3,

  "name": "Chrome Extension Attempt",

  "version": "1.0",

  "description": "News Article Analysis",

  "permissions": [ 

      "activeTab",
      "scripting"  

  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]

}