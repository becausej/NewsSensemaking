{

  "manifest_version": 3,

  "name": "Chrome Extension Attempt",

  "version": "1.0",

  "description": "News Article Analysis",

  "permissions": [ 
      "activeTab",
      "scripting",
      "tabs",
      "storage"
  ],

  "action": {
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "all_frames": false
    }
  ],
  "background": {
    "service_worker": "background.js"
  }

}